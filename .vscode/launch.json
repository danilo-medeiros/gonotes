    "version": "0.2.0",
    "configurations": [
        {
            "name": "Create note",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env": {},
            "args": [
                "--content",
                "This is the content of the note",
                "save"
            ]
        },
        {
            "name": "List notes",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env": {},
            "args": [
                "list"
            ]
        },
        {
            "name": "Delete note",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env": {},
            "args": [
                "--id",
                "1592245759",
                "delete"
            ]
        },
        {
            "name": "Update note",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env": {},
            "args": [
                "--id",
                "1592245759",
                "--content",
                "Updated content",
                "save"
            ]
        },
        {
            "name": "Run webapp",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env": {}
        }
    ]
}